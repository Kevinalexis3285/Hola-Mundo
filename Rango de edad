        "edad = int(input(\"ingrese su edad para saber a que grupo pertenece:\\n\"))\n",
        "if edad >=0 and edad <= 3:\n",
        "    print(f\"{edad} años. Usted pertenece al grupo 'bebe'\")\n",
        "elif edad >=4 and edad <= 12:\n",
        "    print(f\"{edad} años. Usted pertenece al grupo'niño'\")\n",
        "elif edad >=13 and edad <= 17:\n",
        "    print(f\"{edad} años. Usted pertenece al grupo 'adolecente'\")\n",
        "elif edad >=18 and edad <= 60:\n",
        "    print(f\"{edad} años. Usted pertenece al grupo 'adulto'\")\n",
        "elif edad >=61 and edad <= 200:\n",
        "    print(f\"{edad} años. Usted pertenece al grupo'anciano'\")\n",
        "else:\n",
        "    print(f\" {edad}años. Usted pertenece al grupo 'momia'\")\n",
